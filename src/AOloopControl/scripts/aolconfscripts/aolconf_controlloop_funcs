if [[ -f "$file" && ${GPU0ARPFdevice} =~ [0-9]$ ]]; then
echo "OK"
else
echo "0" > $file
GPU0ARPFdevice="0"
fi
fi


if [ "1" -lt "$GPUmodeARPF" ]; then
file="./conf/conf_GPUsetARPF_dev1.txt"
GPU1ARPFdevice=$( cat $file )
if [[ -f "$file" && ${GPU1ARPFdevice} =~ [0-9]$ ]]; then
echo "OK"
else
echo "1" > $file
GPU1ARPFdevice="1"
fi
fi


if [ "2" -lt "$GPUmodeARPF" ]; then
