 echo "OK"
else
 echo "OFF" > ./status/stat_zploopON7.txt
 zploopONstat7="OFF"
fi






GPUzploopONstat0=$( cat ./status/stat_GPUzploopON0.txt )
if [[ -f "./status/stat_GPUzploopON0.txt" && ( "GPU$zploopONstat0" = " ON" || "$GPUzploopONstat0" = "OFF" || "$GPUzploopONstat0" = "WFS" ) ]]; then
