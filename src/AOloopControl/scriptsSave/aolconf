	then
	dm2dmO=$(echo "$(cat $file)")
	else
	dm2dmO="empty" #default
fi
# DM ouput to other loop
aolog "linking dm2dmO to ${dm2dmO}"
rm /tmp/aol${LOOPNUMBER}_dm2dmO.im.shm 
ln -s /tmp/${dm2dmO}.im.shm /tmp/aol${LOOPNUMBER}_dm2dmO.im.shm
}

